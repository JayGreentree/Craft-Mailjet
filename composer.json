{
  "name": "bertoost/craft-mailjet",
  "description": "Craft 3 CMS plugin for adding Mailjet mail adapter.",
  "type": "craft-plugin",
  "version": "1.2.1",
  "keywords": [
    "craft",
    "craftcms",
    "plugin",
    "craftcms plugin",
    "mailjet",
    "mailer",
    "adapter"
  ],
  "homepage": "https://github.com/bertoost/Craft-3-Mailjet",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "craftcms/cms": "^3.1.5",
    "mailjet/mailjet-swiftmailer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "bertoost\\mailjet\\": "src/"
    }
  },
  "extra": {
    "name": "Mailjet",
    "handle": "mailjet",
    "hasCpSettings": true,
    "developer": "Bert Oost",
    "developerUrl": "https://bertoost.com",
    "documentationUrl": "https://github.com/bertoost/Craft-3-Mailjet/blob/master/README.md"
  }
}
